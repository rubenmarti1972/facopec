{
  "name": "facopec-backend",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=18.18.0 <=22",
    "pnpm": ">=8.6.0"
  },
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "predevelop": "node scripts/restore-sql.mjs --if-missing",
    "prestart": "node scripts/auto-seed-if-empty.mjs && node scripts/force-create-admin.mjs",
    "lint": "eslint ./src",
    "seed": "ts-node src/database/seed.ts",
    "seed:production": "NODE_ENV=production SEED_ON_BOOTSTRAP=true ts-node src/database/seed.ts",
    "add-missing-data": "ts-node src/database/add-missing-data.ts",
    "upload-images": "ts-node src/database/upload-images.ts",
    "restore:db": "node scripts/restore-sql.mjs",
    "setup:postgres": "node scripts/setup-postgres.mjs",
    "migrate:postgres": "node scripts/migrate-sqlite-to-postgres.mjs",
    "migrate:verify": "node scripts/migrate-and-verify.mjs",
    "upgrade:stable": "node upgrade-strapi-to-stable.mjs"
  },
  "dependencies": {
    "@strapi/plugin-users-permissions": "^5.30.1",
    "@strapi/provider-email-nodemailer": "^5.31.0",
    "@strapi/provider-upload-cloudinary": "^5.31.2",
    "@strapi/strapi": "5.30.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "11.8.1",
    "pg": "8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.1",
    "styled-components": "6.1.13"
  },
  "devDependencies": {
    "@strapi/types": "5.30.1",
    "@strapi/typescript-utils": "5.30.1",
    "@types/node": "22.10.2",
    "eslint": "9.17.0",
    "prettier": "3.4.2",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  },
  "strapi": {
    "uuid": "c74e21fa-4d0f-4719-a90b-34d981ed9f6a"
  }
}
